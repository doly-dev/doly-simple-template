{
  "name": "doly-simple-template",
  "version": "1.0.0",
  "description": "Development web with doly-cli.",
  "scripts": {
    "start": "doly dev",
    "start:no-mock": "cross-env MOCK=none doly dev",
    "build": "doly build",
